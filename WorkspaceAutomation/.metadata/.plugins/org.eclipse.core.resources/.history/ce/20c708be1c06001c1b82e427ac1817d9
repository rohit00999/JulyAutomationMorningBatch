package miscellaneous;
class Example
{
	static int count = 100;
	
	static void displayName()
	{	
		String s = "Rohit";
		if(count>=0)
		{
			System.out.println(s);
			count = count-1;
			displayName;
		}
	}
}
public class WithoutUseLoopPrintName {
	public static void main(String[] args) {
		
		Example.displayName();

	}
}
